      "source": [
        "t = int(input())\n",
        "for i in range(t):\n",
        "    h,w,n = map(int, input().split())\n",
        "    num = n//h +1\n",
        "    floor = n%h\n",
        "    if n%h==0:\n",
        "        num = n//h\n",
        "        floor = h\n",
        "    print(f'{floor*100+num}')"
      ]
    }
